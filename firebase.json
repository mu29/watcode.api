{
  "hosting": {
    "public": "public",
    "headers": [{
      "source": "**",
      "headers": [{
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }, {
        "key": "Access-Control-Allow-Headers",
        "value": "*"
      }]
    }],
    "rewrites": [{
      "source": "/artworks/*/comments",
      "function": "comments"
    }, {
      "source": "/artworks",
      "function": "artworks"
    }, {
      "source": "/artworks/**",
      "function": "artworks"
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
