{
  "hosting": {
    "public": "public",
    "rewrites": [{
      "source": "/artworks/*/views",
      "function": "views"
    }, {
      "source": "/artworks/*/comments",
      "function": "comments"
    }, {
      "source": "/artworks",
      "function": "artworks"
    }, {
      "source": "/artworks/**",
      "function": "artworks"
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
